  },
  "dependencies": {
    "@supabase/supabase-js": "^2.47.10"
  },
  "devDependencies": {
    "@sedona/eslint-config": "workspace:*",
    "@sedona/typescript-config": "workspace:*",
    "@types/node": "^22.10.5",
    "eslint": "^8.57.1",
    "supabase": "^1.226.4",
    "typescript": "^5.7.2"
  }
}
